{
  "name": "edagenai-portal",
  "description": "Data FlyWheel",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@carbon/react": "^1.92.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.3.2",
    "@mui/material": "^7.0.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@reduxjs/toolkit": "^2.2.1",
    "axios": "^1.8.4",
    "clsx": "^2.1.1",
    "env-cmd": "^10.1.0",
    "highcharts": "^12.4.0",
    "highcharts-react-official": "^3.2.2",
    "i18next": "^23.10.1",
    "i18next-browser-languagedetector": "^7.2.0",
    "lucide-react": "^0.544.0",
    "react": "19.0.0",
    "react-app-polyfill": "^3.0.0",
    "react-dom": "19.0.0",
    "react-helmet-async": "^2.0.4",
    "react-i18next": "^14.1.3",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.22.2",
    "react-scripts": "5.0.1",
    "react-spinners": "^0.17.0",
    "react-syntax-highlighter": "^15.6.1",
    "react-toastify": "^11.0.5",
    "react-vega": "^7.6.0",
    "sass": "^1.85.1",
    "source-map-explorer": "^2.5.3",
    "styled-components": "^6.1.8",
    "tailwind-merge": "^3.3.1",
    "typescript": "^5.3.3",
    "uuid": "^11.1.0",
    "vega": "^6.1.2",
    "vega-lite": "^6.2.0",
    "web-vitals": "^3.5.2"
  },
  "scripts": {
    "tsc:watch": "tsc --watch",
    "prepare": "husky install",
    "clean": "rm -rf build && rm -rf node_modules rm -r package-lock.json",
    "start": "env-cmd -f .env.staging react-scripts start",
    "build:staging": "env-cmd -f .env.staging react-scripts build",
    "build:production": "env-cmd -f .env.production react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint ./src/**/*.{js,ts,tsx}",
    "lint:fix": "eslint --fix ./src/**/*.{js,ts,tsx}",
    "format": "prettier --write ./src/**/*.{js,ts,tsx}",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.0.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.6.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/highcharts": "^5.0.44",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/react-i18next": "^7.8.3",
    "@types/styled-theming": "^2.2.8",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "autoprefixer": "^10.4.21",
    "commitlint": "^19.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss": "^8.5.3",
    "react-refresh": "^0.17.0",
    "tailwindcss": "^3.4.17"
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx,css}": [
      "npm run lint:fix",
      "npm run format",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "subject-case": [
        2,
        "always",
        [
          "sentence-case"
        ]
      ]
    }
  }
}
